generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model sync {
  id         BigInt  @id @default(autoincrement())
  guild_id   String? @db.VarChar(255)
  channel_id String? @db.VarChar(255)
  project_id String? @db.VarChar(255)
  repo_owner String? @db.VarChar(255)
  repo_name  String? @db.VarChar(255)
}
