generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model guilds {
  guild_id   String  @id @db.VarChar(255)
  channel_id String? @db.VarChar(255)
  project_id String? @db.VarChar(255)
  repo_owner String? @db.VarChar(255)
  repo_name  String? @db.VarChar(255)
}
